{
	"name": "obsidianrndvault",
	"version": "0.0.1",
	"description": "Testing and staging for my \"real\" vaults.",
	"type": "commonjs",
	"main": ".",
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1",
		"generate-docs": "node_modules/.bin/jsdoc"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Bogstag/ObsidianRnDVault.git"
	},
	"author": "",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/Bogstag/ObsidianRnDVault/issues"
	},
	"homepage": "https://github.com/Bogstag/ObsidianRnDVault#readme",
	"devDependencies": {
		"@biomejs/biome": "1.3.3",
		"@jsdoc/cli": "^0.3.5",
		"@jsdoc/core": "^0.5.4",
		"@jsdoc/doclet": "^0.2.5",
		"@jsdoc/eslint-config": "^1.2.4",
		"@jsdoc/parse": "^0.3.5",
		"@jsdoc/plugins": "^0.1.5",
		"@jsdoc/prettier-config": "^0.2.3",
		"@jsdoc/tag": "^0.2.5",
		"@jsdoc/task-runner": "^0.2.2",
		"@jsdoc/util": "^0.3.0",
		"docdash": "^2.0.2",
		"jsdoc": "^4.0.2"
	},
	"jsdoc": {
		"docdash": {
			"collapse": true,
			"commonNav": true,
			"menu": {
				"Github repo": {
					"class": "menu-item",
					"href": "https://github.com/Bogstag/ObsidianRnDVault",
					"id": "repository",
					"target": "_blank"
				}
			},
			"meta": {
				"description": "Test and staging repo for my real vaults.",
				"keyword": "",
				"title": "Obsidian RnD Vault"
			},
			"nameInOutputPath": false,
			"navLevel": 2,
			"private": false,
			"removeQuotes": "trim",
			"scopeInOutputPath": false,
			"scripts": "Docs/ScriptsDocs/scripts",
			"search": true,
			"sectionOrder": [
				"Classes",
				"Modules",
				"Externals",
				"Events",
				"Namespaces",
				"Mixins",
				"Tutorials",
				"Interfaces"
			],
			"ShortenTypes": false,
			"sort": true,
			"static": true,
			"versionInOutputPath": false,
			"wrap": true
		},
		"markdown": {
			"hardwrap": true,
			"idInHeadings": true,
			"parser": "gfm"
		},
		"opts": {
			"destination": "Docs/ScriptsDocs",
			"encoding": "utf8",
			"recurse": true,
			"template": "node_modules/docdash",
			"verbose": true
		},
		"plugins": [
			"plugins/markdown",
			"plugins/summarize"
		],
		"recurseDepth": 10,
		"source": {
			"exclude": [
				"ObsidianRnDVault/System/Scripts/secrets"
			],
			"excludePattern": "(^|\\/|\\\\)_",
			"include": [
				"ObsidianRnDVault/System/Scripts/Classes/",
				"ObsidianRnDVault/System/Scripts/Functions/",
				"ObsidianRnDVault/System/Scripts/Utils/",
				"./README.md"
			],
			"includePattern": ".+\\.(js(doc|x)?|cjs)$"
		},
		"sourceType": "commonjs",
		"tags": {
			"allowUnknownTags": true,
			"dictionaries": [
				"jsdoc",
				"closure"
			]
		},
		"templates": {
			"cleverLinks": true,
			"default": {
				"includeDate": false,
				"outputSourceFiles": true,
				"staticFiles": {
					"include": [
						"Docs/ScriptsDocs/static"
					]
				},
				"useLongnameInNav": true
			},
			"monospaceLinks": false
		}
	}
}